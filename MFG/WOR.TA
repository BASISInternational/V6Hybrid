1210:  <F1>=Last",V0=3,V1=40,V2=5
1220 PRINT @(V1,V2),'CL',
1230 GOSUB 7000
1240 IF V3=4 THEN GOTO 9900
1250 IF V3=3 THEN GOSUB 6000
1260 IF V$<>"" AND V3<>1 THEN GOTO 1300
1270 LET OPTHRU$=""
1280 PRINT @(V1,V2),'CL',"Last",
1290 GOTO 1360
1300 LET V$=V$+"   ",V$=V$(1,3)
1310 IF OPFROM$<>"" THEN IF V$<OPFROM$ THEN GOTO 1200
1320 IF P3$(1,1)="Y" THEN FIND (BMM08_DEV,KEY=N0$+V$,DOM=1200)IOL=BMM08A
1330 IF P3$(1,1)<>"Y" THEN FIND (WOM02_DEV,KEY=N0$+V$,DOM=1200)IOL=WOM02A
1340 PRINT @(V1,V2),V$," ",X$(6,20),
1350 LET OPTHRU$=V$
1360 GOTO 3900
1400 REM " --- Starting
1410 LET V0$="D",V1$="C",V2$=DATEFROM$,V3$="",V4$="Enter Starting Date, <F1>=F
1410:irst",V0=6,V1=40,V2=7
1420 PRINT @(V1,V2),'CL',
1430 GOSUB 7000
1440 IF V$<>"" AND V3<>1 THEN GOTO 1480
1450 LET DATEFROM$=""
1460 PRINT @(V1,V2),'CL',"First",
1470 GOTO 1490
1480 LET DATEFROM$=V$
1490 GOTO 3900
1600 REM " --- Ending
1610 LET V0$="D",V1$="C",V2$=DATETHRU$,V3$="",V4$="Enter Ending Date, <F1>=Las
1610:t",V0=6,V1=40,V2=9
1620 PRINT @(V1,V2),'CL',
1630 GOSUB 7000
1640 IF V$<>"" AND V3<>1 THEN GOTO 1675
1650 LET DATETHRU$=""
1660 PRINT @(V1,V2),'CL',"Last",
1670 GOTO 1690
1675 IF DATEFROM$<>"" THEN IF V$<DATEFROM$ THEN GOTO 1600
1680 LET DATETHRU$=V$
1690 GOTO 3900
1800 REM " --- % of Utilization         
1820 LET V0$="N",V1$="C",V2$=STR(UTILPERC),V3$="##0.0",V4$="Enter Minimum Perc
1820:ent of Utilization To Report",V0=3,V1=40,V2=11
1825 PRINT @(V1,V2),'CL',
1830 GOSUB 7000
1840 IF V<0 THEN GOTO 1800
1850 LET UTILPERC=V
1890 GOTO 3900
3900 REM " --- Next Field
3910 IF V3=4 THEN GOTO 4000
3920 IF V3<>2 THEN GOTO 3950
3930 IF I0>0 THEN LET I0=I0-1
3940 GOTO 3960
3950 LET I0=I0+1
3960 ON I0 GOTO 1000,1200,1400,1600,1800,4000
4000 REM " --- Overlay
4020 LET V0$="Y",V1$="C",V2$="Y",V3$="",V4$="Is The Above Information Correct?
4020: (Y/N): ",V0=1,V1=FNV(V4$),V2=22
4030 GOSUB 7000
4040 IF V3=4 THEN GOTO 9900
4060 IF V$="N" THEN GOTO 1000
4080 IF V$<>"Y" THEN GOTO 4000
4190 RUN "WOR.TB"
