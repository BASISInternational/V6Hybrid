1000 REM " --- From Op Code
1010 LET V0$="S",V1$="KEC",V2$=OPFROM$,V3$="",V4$="Enter Valid Operation Code,
1010: <F1>=First, <F3>=Lookup",V0=3,V1=40,V2=3,I0=0
1020 PRINT @(V1,V2),'CL',
1030 GOSUB 7000
1040 IF V3=4 THEN GOTO 9900
1050 IF V3=3 THEN GOSUB 6000
1060 IF V$<>"" AND V3<>1 THEN GOTO 1100
1070 LET OPFROM$=""
1080 PRINT @(V1,V2),'CL',"First",
1090 GOTO 1150
1100 LET V$=V$+"   ",V$=V$(1,3)
1110 IF P3$(1,1)="Y" THEN FIND (BMM08_DEV,KEY=N0$+V$,DOM=1000)IOL=BMM08A
1120 IF P3$(1,1)<>"Y" THEN FIND (WOM02_DEV,KEY=N0$+V$,DOM=1000)IOL=WOM02A
1130 PRINT @(V1,V2),V$," ",X$(6,20),
