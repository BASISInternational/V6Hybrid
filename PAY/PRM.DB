0040 REM 
0085 SETESC 9000
0090 SETERR 9000
0095 PRECISION P[3]
0200 REM " --- IOLists
0220 PRM10G: IOLIST X0$
0230 PRM10HD: IOLIST X0$
2000 REM " --- Description
2020 LET V0$="S",V1$="",V2$=A0$(6,16),V3$="",V0=16,V1=15,V2=4,I0=0
2040 GOSUB 7000
2060 LET A0$(6,16)=V$
2090 GOTO 3900
2100 REM " --- Report Code
2120 LET V0$="Z",V1$="KCR",V2$=A0$(22,2),V0=2,V1=15,V2=5,V4$="Enter Valid Repo
2120:rt Code, <F3>=Code Lookup"
2140 GOSUB 7000
2141 IF V3=2 OR V3=4 THEN GOTO 3900
2142 IF V3<>3 THEN GOTO 2150
2144 LET V4=1,V6=5,V7=2,V8=7,V9=10,V5$="HD"
2146 CALL "SYC.LZ",V1,V2,V4,PRM10_DEV2,V6,V7,V8,V9,V$,V5$,N0$
2148 IF V$="" THEN GOTO 2100
2150 LET A0$(22,2)=V$
2160 FIND (PRM10_DEV2,KEY=N0$+"HD"+A0$(22,2),DOM=2100)IOL=PRM10HD
2170 PRINT @(V0+V1+1,V2),X0$(7,10),
2190 GOTO 3900
2200 REM " --- Cont Type
2220 LET V0$="S",V1$="RC",V2$=A0$(24,1),V3$="FUDN",V4$="F=Fixed, U=Units, D=Do
2220:llars, N=None",V0=1,V1=15,V2=6
2240 GOSUB 7000
2260 LET A0$(24,1)=V$
2290 GOTO 3900
2300 REM " --- Periods Taken
2320 LET V0$="S",V2$=A0$(25,5),V0=5,V1=15,V2=7
2340 GOSUB 7000
2360 LET A0$(25,5)=V$
2390 GOTO 3900
2400 REM " --- Total Code
2420 LET V0$="Z",V2$=A0$(31,2),V0=2,V1=15,V2=9,V1$="KC",V4$="Enter Total Code,
2420: <F3>=Code Lookup"
2430 GOSUB 7000
2435 IF V3=2 THEN GOTO 3900
2440 IF NUM(V$)=0 AND POS(A0$(24,1)="DU")<>0 THEN GOTO 2400 ELSE IF NUM(V$)=0 
2440:THEN LET V$="  "
2442 IF V3<>3 THEN GOTO 2460
2444 GOSUB 6100
2448 IF V$="" THEN GOTO 2400
2460 LET A0$(31,2)=V$
2465 IF A0$(31,2)="  " THEN GOTO 2490
2470 FIND (PRM10_DEV2,KEY=N0$+"G"+A0$(31,2),DOM=2400)IOL=PRM10G
2475 PRINT @(V1+V0+1,V2),X0$(6,20),
2490 GOTO 3900
2500 REM " --- Reg/Premium
2520 LET V0$="S",V1$="RC",V2$=A0$(30,1),V3$="RPB",V4$="R=Regular, P=Premium, B
2520:=Both",V0=1,V1=15,V2=8
2540 GOSUB 7000
2560 LET A0$(30,1)=V$
2590 GOTO 3900
2600 REM " --- Tax Rate
2620 LET V0$="N",V2$=STR(A[0]),V3$=M3$,V1=15,V2=10
2640 GOSUB 7000
2660 LET A[0]=V
2690 GOTO 3900
2700 REM " --- Min Limit
2720 LET V0$="N",V2$=STR(A[1]),V3$=M1$,V1=15,V2=11
2740 GOSUB 7000
2760 LET A[1]=V
2790 GOTO 3900
2800 REM " --- Max Limit
2820 LET V0$="N",V2$=STR(A[2]),V3$=M1$,V1=15,V2=12
2840 GOSUB 7000
2860 LET A[2]=V
2890 GOTO 3900
3100 REM " --- Total To
3110 FOR X=0 TO 4
3120 LET V0$="Z",V1$="KC",V2$=A1$(X*2+1,2),V0=2,V1=15,V2=X+15
3125 LET V4$="Enter A Valid Total Code, <F3>=Lookup"
3130 GOSUB 7000
3132 IF V3<>3 THEN GOTO 3140
3134 GOSUB 6100
3136 IF V$="" THEN GOTO 3120
3140 LET A1$(X*2+1,2)=V$
3150 IF V3=4 THEN EXITTO 3900
3160 IF V3<>2 THEN GOTO 3192
3170 IF X=0 THEN EXITTO 3900
3180 LET X=X-1
3190 GOTO 3120
3192 FIND (PRM10_DEV2,KEY=N0$+"G"+A1$(X*2+1,2),DOM=3120)IOL=PRM10G
3194 PRINT @(V1+3,V2),X0$(6,20),
3200 REM " --- Operator
3220 LET V0$="S",V1$="RC",V2$=A2$(X+1,1),V3$="+- ",V4$="+ Or -",V0=1,V1=45,V2=
3220:X+15
3230 GOSUB 7000
3240 LET A2$(X+1,1)=V$
3250 IF V3=4 THEN EXITTO 3900
3260 IF V3=2 THEN GOTO 3120
3270 NEXT X
3290 GOTO 3900
3300 REM " --- Special Tax
3320 LET V0$="Y",V1$="RC",V2$=A0$(33,1),V4$="Y=Assign Tax Individually, N=Assi
3320:gn Tax To All Employees",V0=1,V1=60,V2=6
3340 GOSUB 7000
3360 LET A0$(33,1)=V$
3390 GOTO 3900
3900 REM " --- Next Field
3910 IF V3=4 THEN GOTO 4000
3920 IF V3<>2 THEN GOTO 3950
3930 IF I0>0 THEN LET I0=I0-1
3940 GOTO 3960
3950 LET I0=I0+1
3960 ON I0 GOTO 2000,2100,2200,2300,2500,2400,2600,2700,2800,3300,3100,3990
3990 LET I0=K0
4000 REM " --- Overlay
4010 GOSUB 6500
4020 IF F9$<>"1" THEN GOTO 6400
4030 RUN "PRM.DA"
6000 REM " --- Subroutines
6100 REM " --- Total Code Lookup
6110 LET V4=1,V6=4,V7=2,V8=6,V9=20,V5$="G"
6120 CALL "SYC.LZ",V1,V2,V4,PRM10_DEV2,V6,V7,V8,V9,V$,V5$,N0$
6130 RETURN
6400 REM " --- Messages
6410 DIM MESSAGES$[0]
6460 LET MESSAGES$[0]="Report Code Is Required, <Enter>=Re-Enter, <F4>=Exit: "
6470 CALL "SYC.XA",0,MESSAGES$[ALL],0,22,-1,V$,V3
6480 IF V3=4 THEN GOTO 6600
6490 GOTO 3960
6500 REM " --- Verify
6510 LET F9$=""
6520 FIND (PRM10_DEV2,KEY=N0$+"HD"+A0$(22,2),DOM=6540)
6530 LET F9$="1"
6590 RETURN
6600 REM " --- Exit
6610 DIM MESSAGES$[0]
6620 LET MESSAGES$[0]="This Contribution Code Will Be Deleted, <Enter>: "
6640 CALL "SYC.XA",0,MESSAGES$[ALL],0,22,-1,V$,V3
6650 LET O7=1
6690 GOTO 4030
7000 REM " --- Standard Input Routine (15May95)"
7010 CALL "SYC.IA",V0$,V1$,V2$,V3$,V4$,PGM(-2),V0,V1,V2,V$,V,V3,DISPLAY_LEN
7020 IF V3=999 THEN GOTO 7200
7030 IF V3=127 THEN GOTO 7100
7090 RETURN
7100 REM " --- Escape During Input"
7110 CALL "SYC.ES",ERR=7200,PGM(-2),TCB(8),E$,E2,V3
7120 IF V3<>127 THEN GOTO 7000
7130 PRINT @(0,E2),'CL','CURSOR'("ON"),E$,'LF'
7200 REM " --- Error During Input"
7210 ESCAPE
7290 GOTO 7000
8000 REM " --- Functions
8200 REM " --- FNF$ AlphaNumeric Masking Function"
8210 DEF FNF$(Q1$,Q2$)
8220 IF Q2$="" THEN LET Q2$=FILL(LEN(Q1$),"0")
8230 RETURN STR(-NUM(Q1$,ERR=ALPHA_FNF):Q2$,ERR=ALPHA_FNF)
8240 ALPHA_FNF:
8245 LET Q=1,Q0=0
8250 WHILE LEN(Q2$(Q))
8255 IF POS(Q2$(Q,1)="-()") THEN LET Q0=Q0+1 ELSE LET Q2$(Q,1)="X"
8260 LET Q=Q+1
8265 WEND
8270 IF LEN(Q1$)>LEN(Q2$)-Q0 THEN LET Q1$=Q1$(1,LEN(Q2$)-Q0)
8280 RETURN STR(Q1$:Q2$)
8290 FNEND
9000 REM " --- Standard Error Routine (15May95)"
9010 IF ERR=127 THEN GOTO 9300
9020 CALL "SYC.EA",ERR=9090,EXIT_CTRL,PGM(-2),ERR,TCB(5),E$,E1,E2
9030 IF E1=1 THEN GOTO 9100
9040 IF E1=3 THEN GOTO 9200
9080 PRINT @(0,E2),'CL','CURSOR'("ON"),E$,'LF'
9090 ESCAPE
9100 REM " --- Retry"
9190 RETRY
9200 REM " --- Return"
9290 GOTO 9900
9300 REM " --- Standard Escape Routine"
9310 CALL "SYC.ES",ERR=9350,PGM(-2),TCB(8),E$,E2,V3
9320 IF V3<>127 THEN GOTO 9390
9330 PRINT @(0,E2),'CL','CURSOR'("ON"),E$,'LF'
9350 ESCAPE
9390 RETURN
9900 RUN "SYS.AA"
9999 END
