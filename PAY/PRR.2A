0740 PRINT 'SB',@(29,3),"From Date:",@(29,5),"Thru Date:",@(25,7),"From Employ
0740:ee:",@(25,9),"Thru Employee:",'SF',
1000 REM " --- From Date
1005 PRINT 'CF',
1010 LET V0$="D",V1$="EC",V2$=FROMDATE$,V4$="Enter Starting Date, <F1> For Fir
1010:st",V0=8,V1=40,V2=3,I0=0
1020 GOSUB 7000
