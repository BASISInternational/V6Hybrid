1040 IF V$="" OR V3=1 THEN PRINT @(40,3),"First Date",'CL',; LET FROMDATE$="";
1040: GOTO 1090
1050 LET FROMDATE$=V$
1060 IF POS(" "<>FROMDATE$)=0 THEN LET V$=""; GOTO 1040
1090 GOTO 3900
1100 REM " --- Thru Date
