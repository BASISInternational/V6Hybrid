3900 REM " --- Next Field
3910 IF V3<>2 THEN GOTO 3940
3920 IF I0>0 THEN LET I0=I0-1
3930 GOTO 3950
3940 LET I0=I0+1
3960 ON I0 GOTO 1000,1100,1200,1300,3990
4000 REM " --- Options
4010 LET V0$="Y",V1$="CE",V2$="",V3$="",V4$="Is The Above Information Correct 
4010:(Y/N)?",V0=1,V1=FNV(V4$),V2=22
4020 GOSUB 7000
4030 IF V$="N" THEN GOTO 1000
4040 IF V3=4 THEN GOTO 9900
