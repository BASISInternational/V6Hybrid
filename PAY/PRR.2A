1355 IF X1$<X0$ THEN GOTO 1300
1360 FIND (PRM01_DEV,KEY=N0$+X1$,DOM=1300)IOL=PRM01A
1370 PRINT @(V1,V2),FNF$(V$(1,P[0]),M0$)," ",X$(1,30),
1380 LET THRUEMPL$=V$
