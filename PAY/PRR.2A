1110:,V0=8,V1=40,V2=5
1120 GOSUB 7000
1130 IF V3=4 THEN GOTO 9900
1140 IF V$="" OR V3=1 THEN PRINT @(40,5),"Last Date",'CL',; LET THRUDATE$=""; 
1140:GOTO 1190
1150 LET THRUDATE$=V$
1160 IF THRUDATE$<FROMDATE$ THEN GOTO 1100
1170 IF POS(" "<>THRUDATE$)=0 THEN LET V$=""; GOTO 1140
1190 GOTO 3900
1200 REM " --- From Employee
1210 LET V0$="M",V1$="EC",V2$=FROMEMPL$,V3$=M0$,V4$="Enter Employee Number, <F
1210:1>=First, <F3>=Lookup",V0=P[0],V1=40,V2=7
1220 GOSUB 7000
