1240:O 1290
1250 LET X0$=V$+FILL(9-LEN(V$),"0")
1260 FIND (PRM01_DEV,KEY=N0$+X0$,DOM=1200)IOL=PRM01A
1270 PRINT @(V1,V2),FNF$(V$(1,P[0]),M0$)," ",X$(1,30),
1280 LET FROMEMPL$=V$
1290 GOTO 3900
1300 REM " --- Thru Employee
1310 LET V0$="M",V1$="EC",V2$=THRUEMPL$,V3$=M0$,V4$="Enter Employee Number, <F
1310:1>=Last, <F3>=Lookup",V0=P[0],V1=40,V2=9
1320 GOSUB 7000
1330 IF V3=4 THEN GOTO 9900
1335 IF V3=3 THEN CALL "SYC.LP",V$
