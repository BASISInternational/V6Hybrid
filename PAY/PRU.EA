3150 PRINT @(COLUMN,11),C0$(3,4),
3160 FIND (PRM01_DEV,KEY=N0$+C0$(7,9),DOM=3300)
3170 FOR X0=0 TO 1
3180 LET C[6*X0+1]=0,C[6*X0+2]=0,C[6*X0+3]=0,C[6*X0+4]=0
3190 NEXT X0
3200 FOR X0=0 TO 11
3220 IF C[X0]<>0 THEN EXITTO 3280
3230 NEXT X0
