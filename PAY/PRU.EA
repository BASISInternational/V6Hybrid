3280 WRITE (PRT05_DEV,KEY=C0$)IOL=PRT05A
3290 GOTO 3100
3300 REMOVE (PRT05_DEV,KEY=C0$,DOM=3310)
3390 GOTO 3100
4000 REM " --- Done"
4020 GOTO 9900
6000 REM " --- Clear Terminated"
6005 PRINT @(0,10),'CE',
