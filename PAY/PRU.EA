6225 IF POS(K$=K1$)<>1 THEN GOTO 6250
6230 REMOVE (PRT01_DEV,KEY=K1$)
6240 GOTO 6220
6250 REM " --- Remove Deduction History"
6260 READ (PRT11_DEV,KEY=K$,DOM=6270)
6270 LET K1$=KEY(PRT11_DEV,END=6300)
