1660 LET Y0=Y0+Y[X],Y1=Y1+Y[6+X],Y2=Y2+Y[12+X]
1670 LET V[0]=V[0]+Y[X],V[1]=V[1]+Y[6+X],V[2]=V[2]+Y[12+X]
1680 NEXT X
2000 REM " --- Print"
2010 FOR X=0 TO 2
2012 IF Y[X*6]<>0 THEN IF X*6+P9<>0 THEN EXITTO 2020
2014 NEXT X
2016 IF Y0=0 THEN IF Y1=0 THEN IF Y2=0 THEN GOTO 1400
2020 IF L+3>L9 THEN GOSUB 5000
2040 PRINT (7)@(0),FNF$(B0$(3,P[0]),M0$),@(1+M0),Y[0]:M1$,Y[6]:M1$,Y[12]:M1$,Y
2040:[P9]:M1$,Y[6+P9]:M1$,Y[12+P9]:M1$,Y0:M1$,Y1:M1$,Y2:M1$
2050 PRINT (7)@(2),X1$,@(32),"(",FNF$(B1$(136,9),M9$),")",'LF'
