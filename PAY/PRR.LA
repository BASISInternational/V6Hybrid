1130 IF K0$(1,3)<>N0$+"D" THEN GOTO 4000
1140 READ (PRM10_DEV)IOL=PRM10D
1160 PRINT @(COLUMN,11),A0$(6,16),
1170 IF P8$<>"" THEN IF P8$<>A0$(4,2) THEN GOTO 1100
1180 LET L=L9+1,L1=0
1190 READ (PRM01_DEV,KEY=N0$,DOM=1200)
1400 REM " --- Employee"
