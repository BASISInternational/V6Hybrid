0860 LET K$=KEY(PRM10_DEV,END=0980)
0870 IF K$(1,3)<>N0$+"H" THEN GOTO 0980
0880 READ (PRM10_DEV)IOL=PRM10H
0885 IF K$(4,1)>"C" THEN GOTO 0895
0890 LET U0$=U0$+X0$(4,3),U2$=U2$+X0$(17,2)
0895 IF K$(4,1)<>"F" THEN GOTO 0860
0897 LET U3$=U3$+X0$(5,2),X$=X0$(7,10)
0900 FOR X=1 TO 10
0910 IF X$(10,1)<>" " THEN EXITTO 0950
0920 LET X$=" "+X$(1,9)
0930 NEXT X
0950 LET U1$=U1$+X$
0960 GOTO 0860
0980 DIM X$(80)
0990 LET X$(1)=U1$,X$(71)="     Total",U1$=X$
1000 REM " --- Init Read
1010 PRECISION P[4]
1020 READ (PRE01_DEV,KEY=N0$,DOM=1030)
1100 REM " --- Main Read
1120 LET K$=KEY(PRE01_DEV,END=4000)
1130 IF K$(1,2)<>N0$ THEN GOTO 4000
1140 READ (PRE01_DEV)IOL=PRE01A
1180 PRINT @(COLUMN,11),FNF$(A0$(3,P[0]),M0$),
1400 REM " --- Get Name
1410 LET X1$="Not On File"
1420 DIM B1$(250)
1450 FIND (PRM01_DEV,KEY=A0$(1,11),DOM=1470)IOL=PRM01A
1455 LET Q=POS("  "=B1$(1,16))-1
1457 IF Q<1 THEN LET Q=16
1460 LET X1$=B1$(1,Q)+", "+B1$(17,1)+"."
1500 REM " --- Breaks
1520 IF K$(3,P[3])<>T0$ THEN GOSUB 6000
1600 REM " --- Print Heading
1620 IF L+2>L9 THEN GOSUB 5000
1640 PRINT (7)@(0),FNF$(A0$(3,P[0]),M0$),
1660 IF A0$(12,1)<>"C" THEN PRINT (7)@(M0+8*M1),A0$(12,1),A0$(15,7),
1800 REM " --- Detail
1805 DIM X[8]
1810 REM " --- Earnings
1820 READ (PRE11_DEV,KEY=A0$(1,21),DOM=1830)
1830 LET K11$=KEY(PRE11_DEV,END=1910)
1840 IF K11$(1,21)<>A0$(1,21) THEN GOTO 1900
1850 READ (PRE11_DEV)IOL=PRE11A
1860 GOSUB 6300
1870 GOTO 1830
1910 REM " --- Deductions 
1920 READ (PRE21_DEV,KEY=A0$(1,21),DOM=1930)
1930 LET K21$=KEY(PRE21_DEV,END=2010)
1940 IF K21$(1,21)<>A0$(1,21) THEN GOTO 2010
1950 READ (PRE21_DEV)IOL=PRE21A
1960 GOSUB 6300
1970 GOTO 1930
2010 REM " --- Taxes
2020 READ (PRE31_DEV,KEY=A0$(1,21),DOM=2030)
2030 LET K31$=KEY(PRE31_DEV,END=2500)
2040 IF K31$(1,21)<>A0$(1,21) THEN GOTO 2500
2050 READ (PRE31_DEV)IOL=PRE31A
2060 GOSUB 6300
2070 GOTO 2030
2500 REM " --- Print
2505 IF A0$(12,1)<>"C" THEN GOTO 2520
2510 IF X[0]=0 THEN IF X[1]=0 THEN IF X[2]=0 THEN IF X[3]=0 THEN IF X[4]=0 THE
2510:N IF X[5]=0 THEN IF X[6]=0 THEN IF X[7]=0 THEN GOTO 1100
2520 IF L+2>L9 THEN GOSUB 5000
2525 PRINT (7)@(0),FNF$(A0$(3,P[0]),M0$),
2530 FOR F=1 TO 7
2540 IF X[F]<>0 THEN PRINT (7)@(M0+(F-1)*M1),X[F]:M1$,
2550 NEXT F
2570 PRINT (7)@(M0+7*M1),X[8]:M1$
2580 PRINT (7)@(2),X1$,
2585 IF A0$(12,1)="C" AND X[8]<0 THEN PRINT (7)"  ***** Negative Check Amount 
2585:- Correct And Re-Run Pre-Check Report "; LET X9$="N",N$=N$+A0$(3,P[0])+A0
2585:$(13,2)+X1$+FILL(20-LEN(X1$)),N[LEN(N$)/(P[0]+22)-1]=X[8] ELSE PRINT (7)'
2585:LF',
2590 LET L=L+2,L8=L8+1
2600 GOTO 1100
4000 REM " --- Totals
4020 LET T9$="E"
4040 GOSUB 6000
4060 PRINT (7)'LF',@(5),"Grand Totals:",@(M0),T[1]:M1$,T[2]:M1$,T[3]:M1$,T[4]:
4060:M1$,T[5]:M1$,T[6]:M1$,T[7]:M1$,T[8]:M1$
4100 REM " --- Report Negative check Amount
4110 IF X9$="Y" THEN GOTO 4200
4120 LET HEADINGS$[3]="Negative Check Amount Exception Report"
4130 LET HEADINGS=4,O9=1
4150 GOSUB 4400
4160 LET X=P[0]+22
4170 FOR I=1 TO LEN(N$) STEP X
4175 IF L+1>L9 THEN GOSUB 4400
4180 PRINT (7)@(0),FNF$(N$(I,P[0]),M0$),"  ",N$(I+P[0]+2,20),@(LEN(M0$)+25),N$
4180:(I+P[0],2),@(M0+7*M1),N[(I-1+P[0]+22)/X-1]:M1$
4185 LET L=L+1
4190 NEXT I
4200 REM " --- Set Neg Check Flag
4220 READ (SYS01_DEV,KEY=N0$+N2$+"01",DOM=4300)IOL=PRS01B1
4230 LET D1$(21,1)=X9$
4240 WRITE (SYS01_DEV,KEY=D0$)IOL=PRS01B1
4300 GOTO 9900
4400 REM " --- Negative Check Sub-Heading Routine
4410 GOSUB 5000
4420 PRINT (7)@(0),"Number  Name                  Check Seq Number",
4430 PRINT (7)@(M0+8*M1-6),"Total",'LF'
4440 LET L=L+2
4490 RETURN
5000 REM " --- Report Heading
5010 LET L=HEADINGS+1
5020 CALL "SYC.HA",7,HEADINGS$[ALL],HEADINGS,H,H0,WHEN$,CLOCK$,STATUS
5030 IF STATUS>0 THEN EXITTO 9900
5100 PRINT (7)"Employee"
5105 LET L=L+1
5110 IF O9 THEN GOTO 5290
5120 PRINT (7)"Number & Name",
5150 FOR X=1 TO LEN(U1$)/10
5160 PRINT (7)@(M0-11+M1*X),U1$(10*(X-1)+1,10),
5170 NEXT X
5180 PRINT (7)'LF',
5190 LET L=L+1
5200 REM " --- Sub Heading
5220 IF P[3]=0 THEN RETURN
5240 PRINT (7)'LF',@(0),T0$," ",H3$,'LF'
5280 LET L=L+3
5290 RETURN
6000 REM " --- Dept Break
6020 IF T0$="" THEN GOTO 6100
6030 IF P[3]=0 OR L8<2 THEN GOTO 6100
6040 PRINT (7)'LF',@(5),"Dept Totals:",@(M0),R[1]:M1$,R[2]:M1$,R[3]:M1$,R[4]:M
6040:1$,R[5]:M1$,R[6]:M1$,R[7]:M1$,R[8]:M1$
6070 LET L=L+2
6100 IF T9$="E" THEN RETURN
6120 DIM R[8],S[8]
6140 LET T0$=K$(3,P[3]),L8=0
6150 IF P[3]=0 THEN RETURN
6160 LET H3$=""
6170 FIND (PRM10_DEV,KEY=N0$+"E"+B1$(221,2),DOM=6200)IOL=PRM10E
6180 LET H3$=H3$(6,20)
6200 IF L>L9-4 THEN GOTO 6250
6220 GOSUB 5200
6230 RETURN
6250 GOSUB 5000
6290 RETURN
6300 REM " --- 
6360 LET X=0,X0=-1
6370 FIND (PRM10_DEV,KEY=N0$+W0$(22,1)+W0$(25,2),DOM=6500)IOL=PRM10ABC
6380 LET X=POS(W0$(22,1)+X$(22,2)=U0$)
6390 IF X=0 THEN GOTO 6500
6400 LET X$=U2$(INT(X/3)*2+1,2)
6420 LET X=POS(X$=U3$)
6440 IF X>0 THEN LET X=INT(X/2)+1
6500 IF X>7 THEN LET X=1
6505 IF X=0 THEN LET X=1
6510 IF W0$(22,1)="A" THEN LET X0=1
6520 LET X[X]=X[X]+W[2],T[X]=T[X]+W[2],R[X]=R[X]+W[2]
6540 LET X[8]=X[8]+X0*W[2],T[8]=T[8]+X0*W[2],R[8]=R[8]+X0*W[2]
6590 RETURN
8000 REM " --- Functions
8010 DEF FNA$(Q$,Q2$)=STR(MOD((ASC(Q$)-32)*POS(" "<>Q2$(2,1)),100):"00")
8020 DEF FNB$(Q1$)=FNA$(Q1$(2),Q1$)+"/"+FNA$(Q1$(3),Q1$)+"/"+FNA$(Q1$(1),Q1$)
8200 REM " --- FNF$ AlphaNumeric Masking Function"
8210 DEF FNF$(Q1$,Q2$)
8220 IF Q2$="" THEN LET Q2$=FILL(LEN(Q1$),"0")
8230 RETURN STR(-NUM(Q1$,ERR=ALPHA_FNF):Q2$,ERR=ALPHA_FNF)
8240 ALPHA_FNF:
8245 LET Q=1,Q0=0
8250 WHILE LEN(Q2$(Q))
8255 IF POS(Q2$(Q,1)="-()") THEN LET Q0=Q0+1 ELSE LET Q2$(Q,1)="X"
8260 LET Q=Q+1
8265 WEND
8270 IF LEN(Q1$)>LEN(Q2$)-Q0 THEN LET Q1$=Q1$(1,LEN(Q2$)-Q0)
8280 RETURN STR(Q1$:Q2$)
8290 FNEND
