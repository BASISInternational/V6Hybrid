0515 DIM U[7],S[7],Y[7],W[2],HEADINGS$[4]
0520 LET H0=131,H=0,HEADINGS=3,L9=59,L=L9+1,WHEN$=F0$(7,3),CLOCK$=""
0530 LET HEADINGS$[0]=N1$,HEADINGS$[1]=N3$,HEADINGS$[2]=H4$
0545 CALL "SYC.RM",M1$,M1,8,13
0570 LET M0=20,P7$="E",N$="",X9$="Y",P[4]=NUM(P3$(7,1))
0700 REM " --- Run Report?"
0710 LET V4$="Do You Want To Print The "+N3$
